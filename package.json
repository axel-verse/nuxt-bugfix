{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && npx gzipper c -v --level 9 -e nojekyll,jpg,jpeg,webp,png,svg,woff2,woff,ico,txt ./dist",
    "generate:modern": "nuxt generate --modern && npx gzipper c -v --level 9 -e nojekyll,jpg,jpeg,webp,png,svg,woff2,woff,ico,txt ./dist",
    "generate-deploy": "nuxt generate && npx gzipper c -v --level 9 -e nojekyll,jpg,jpeg,webp,png,svg,woff2,woff,ico,txt ./dist && mcli cp -r dist/ prod/blog/",
    "generate-deploy:modern": "nuxt generate --modern && npx gzipper c -v --level 9 -e nojekyll,jpg,jpeg,webp,png,svg,woff2,woff,ico,txt ./dist && mcli cp -r dist/ prod/blog/",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@nuxt/image": "^0.0.4",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gzipper": "^4.3.0",
    "node-sass": "^5.0.0",
    "postcss": "^7",
    "prettier": "^2.1.2",
    "sass-loader": "^10.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
}
